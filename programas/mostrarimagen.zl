Configuracion
  fps <- 60
	integrar "bibliotecasZL/canvas.zl"
	integrar "bibliotecasZL/basico.zl"
Fin

Subrutina Inicio
Datos
	url es Texto
Algoritmo
	fondo [
	  fondo <- "blanco" como Color
	]
  mostrar [
    mensaje <- "Escribe el url de una imagen:"
  ]
  leer [
    mensaje -> url
  ]
  imagen [
	  url <- url
	  esquina <- "0,0" como Punto
	  ancho <- 320
	  alto <- 320
	]
Fin
